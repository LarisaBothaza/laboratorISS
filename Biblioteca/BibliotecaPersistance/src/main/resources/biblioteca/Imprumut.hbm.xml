<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "biblioteca.Imprumut"
           table="Imprumut">
        <id name = "id" column = "id">
            <generator class="increment"/>
        </id>
        <property name="idCarte" column = "ID_CARTE"/>
        <property name="idAbonat" column="ID_ABONAT"/>
        <property name="dataImprumut" type="date" column="DATA_IMPRUMUT"/>
        <property name="returnat" column="RETURNAT"/>
    </class>

</hibernate-mapping>